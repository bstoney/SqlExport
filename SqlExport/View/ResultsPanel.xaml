<UserControl 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    x:Class="SqlExport.View.ResultsPanel"
    x:Name="UserControl"
    d:DesignWidth="640" d:DesignHeight="480"
    DataContext="{Binding ResultsPanelViewModel, Source={StaticResource Locator}}">
    <Grid Name="LayoutRoot">
        <TabControl x:Name="Results" ItemsSource="{Binding DisplayPages}" SelectedItem="{Binding SelectedTabItem}">
        </TabControl>
    </Grid>
</UserControl>
